{{$chan := getChannel .Channel.ID}}
{{$nchan := str $chan.ID}}
{{if .CmdArgs}}
  {{$chan = index .CmdArgs 0|reFind `\d{17,}`|getChannel}}
  {{$nchan = str $chan.ID}}
{{end}}
  {{$chan = title $chan.Name}}
  {{$chan = reReplace `-` $chan " "}}
  {{$mdic := sdict}}
  {{with (dbGet 0 "room_msg")}}
    {{$mdic = sdict .Value}}
  {{end}}
  {{$embed := cembed
    "title" (print $chan)
    "description" (joinStr " " "Personnes pr√©sentes : \n Cette salle est vide..." )
    "color" 0x8c385f
    "timestamp" .Current.Time
    "thumbnail" (sdict "url" "https://i.imgur.com/bz8YW3y.png")}}
  {{$id := sendMessageRetID (toInt $nchan) $embed}}
  {{$mdic.Set $nchan $id}}
  {{dbSet 0 "room_msg" $mdic}}
{{deleteTrigger 1}}